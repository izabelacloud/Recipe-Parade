<div class="tags">
  {{#each this}}
    <div class="card">
        <h3 class="card-header"></h3>
        <section class="tag card-body">
            <div class="text">
                {{tag_name}}
            </div>
            <div class="meta">
                -{{user.username}}, {{format_date created_at}}
            </div>
        </section>
    </div>
  {{/each}}
</div>